{
  "name": "node-started",
  "version": "0.2.5",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": ">= 12.x"
  },
  "main": "index.js",
  "scripts": {
    "webpack-watch": "webpack --watch",
