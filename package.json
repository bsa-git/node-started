    "dotenv-webpack": "^8.0.1",
    "ejs": "^2.5.5",
    "express": "^4.16.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.2.3",
    "nodemon": "^1.18.1",
    "serve-favicon": "^2.5.0"
